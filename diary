# @(#) diary v1.0	Maintain a diary

#DIARY="$HOME/.diary"
DIARY="$HOME/Dropbox/Personal/Documents/Notes/.diary"
#
# Sync by copying last edited .diary in dropbox to $HOME/.diary
#
#    cp /Users/woo/Dropbox/Personal/Documents/Notes/.diary $DIARY

	echo "----`date`---" >>$DIARY
	echo "" >>$DIARY
	vi + $DIARY
